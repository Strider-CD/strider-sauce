{
  "author": "Niall O'Higgins <niallo@frozenridge.co> (http://frozenridge.co)",
  "description": "Sauce Labs / Selenium support for Strider",
  "name": "strider-sauce",
  "version": "0.5.1",
  "homepage": "http://github.com/Strider-CD/strider-sauce",
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider-sauce.git"
  },
  "dependencies": {
    "request": "~2.11.4",
    "wd": "0.0.32",
    "httpcheck": "0.0.1"
  },
  "devDependencies": {},
  "optionalDependencies": {},
  "keywords": [
    "strider",
    "sauce",
    "selenium",
    "tests"
  ],
  "strider": {
    "id": "sauce",
    "type": "job",
    "title": "Sauce",
    "worker":"worker.js",
    "webapp":"webapp.js"
  },
  "engines": {
    "node": "*"
  },
  "strider": {
    "webapp":"./webapp.js",
    "panel" : {
      "src": "./templates/project_config.html",
      "controller": "SauceCtrl",
      "title": "Sauce Config",
      "id": "strider-sauce",
      "data": ["sauce_browsers", "sauce_access_key", "sauce_password"]
    }
  }
}
