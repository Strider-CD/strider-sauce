{
  "name": "strider-sauce",
  "version": "0.6.3",
  "description": "Sauce Labs / Selenium support for Strider",
  "homepage": "http://github.com/Strider-CD/strider-sauce",
  "author": "Niall O'Higgins <niallo@frozenridge.co> (http://frozenridge.co)",
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider-sauce.git"
  },
  "dependencies": {
    "request": "~2.79.0",
    "wd": "1.1.3",
    "httpcheck": "0.3.0",
    "unzip": "~0.1.9"
  },
  "devDependencies": {
    "jshint": "~2.9.4",
    "mocha": "~3.2.0"
  },
  "scripts": {
    "test": "make test",
    "install": "node install.js"
  },
  "keywords": [
    "strider",
    "sauce",
    "selenium",
    "tests",
    "saucelabs"
  ],
  "strider": {
    "id": "sauce",
    "type": "job",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "title": "Sauce Labs",
    "config": {
      "controller": "SauceCtrl"
    }
  },
  "engines": {
    "node": "*"
  }
}
