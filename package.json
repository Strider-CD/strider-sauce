{
  "author": "Niall O'Higgins <niallo@frozenridge.co> (http://frozenridge.co)",
  "description": "Sauce Labs / Selenium support for Strider",
  "name": "strider-sauce",
  "version": "0.6.1",
  "homepage": "http://github.com/Strider-CD/strider-sauce",
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider-sauce.git"
  },
  "dependencies": {
    "request": "~2.11.4",
    "wd": "0.0.32",
    "httpcheck": "0.0.1",
    "unzip": "~0.1.9"
  },
  "devDependencies": {
    "jshint": "~2.3.0",
    "mocha": "~1.14.0"
  },
  "optionalDependencies": {},
  "scripts": {
    "test": "make test",
    "install": "node install.js"
  },
  "keywords": [
    "strider",
    "sauce",
    "selenium",
    "tests"
  ],
  "strider": {
    "id": "sauce",
    "type": "job",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "title": "Sauce Labs",
    "config": {
      "controller": "SauceCtrl"
    }
  },
  "engines": {
    "node": "*"
  }
